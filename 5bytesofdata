DATA SEGMENT
ARRAY1 DB 0F0H,20H,30H,40H,50H,60H,70H,80H 
RESULT DW 0 
DATA  ENDS 
STACK_SEG  ENDS 
CODE  SEGMENT
DB. 100 DUP(0)
STACK_SEG ENDS 
CODE  SEGMENT
ASSUME CS: CODE, DS: DATA ,SS: STACK_SEG 
START :MOV AX,DATA;
       MOV DS,AX;
       MOV AX,STACK_SEG ;
       MOV SS,AX;
       CALL ARRADD;
       MOV RESULT,AX ;
LAST:  MOV AH,4CH ;
       INT 21H ;
ARRADD  PROC  NEAR 
       LEA BX,ARRAY1;
       MOV CX, 08;
       MOV AH,00;
       MOV AL,00;
       ADD AL,[BX];
       JNC  SKIP ;
       INC  AH;
SKIP:  INC  BX ;
       LOOP BACK ;
       RET ;
ENDP 
CODE ENDS 
END START

